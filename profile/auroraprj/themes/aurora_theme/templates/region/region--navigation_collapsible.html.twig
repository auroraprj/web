{#
/**
 * @file
 * Implementación de la zona de navegación plegable
 *
 * @ingroup templates
 * @see template_preprocess_region()
 */
#}
{%
  set classes = [
    'collapse',
    'navbar-collapse',
    'region-' ~ region|clean_class,
  ]
%}
{% if content %}
  <div{{ attributes.addClass(classes).setAttribute('id','navbar-collapse') }}>
    {{ content }}
  </div>
{% endif %}
